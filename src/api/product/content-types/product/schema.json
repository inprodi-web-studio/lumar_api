{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "uuid": {
      "type": "string"
    },
    "sku": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "unity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::unity.unity",
      "inversedBy": "products"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "mp",
        "sp",
        "pt"
      ]
    },
    "saleInfo": {
      "displayName": "saleInfo",
      "type": "component",
      "repeatable": false,
      "component": "product-properties.sale-info"
    },
    "purchaseInfo": {
      "displayName": "Purchase Info",
      "type": "component",
      "repeatable": false,
      "component": "product-properties.purchase-info"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "products"
    },
    "inventoryInfo": {
      "displayName": "Inventory Info",
      "type": "component",
      "repeatable": false,
      "component": "product-properties.inventory-info"
    },
    "materials": {
      "type": "json"
    }
  }
}
